#! /usr/bin/env python
# 2012-2013 Ford Peprah
# A python script that acts as a wrapper for calling the Marmoset 
# submission script.

if __name__ == "__main__":
    try:
        import marmoset.marmoset as marmoset
    except ImportError:
        import marmoset as marmoset

    import sys

    marmoset.main(sys.argv)
